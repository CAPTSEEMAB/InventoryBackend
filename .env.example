# ===========================================

# INVENTORY MANAGEMENT API CONFIGURATION

# ===========================================# AWS Cognito Configuration  

# Copy this file to .env and fill in your actual valuesAWS_COGNITO_USER_POOL_ID=your_cognito_user_pool_id

AWS_COGNITO_CLIENT_ID=your_cognito_client_id

# Server ConfigurationAWS_COGNITO_REGION=us-east-1

PORT=8000

API_PREFIX=/api# AWS SNS Configuration

ENVIRONMENT=developmentAWS_SNS_REGION=us-east-1

SNS_ENABLE_NOTIFICATIONS=true

# AWS Global Configuration

AWS_ACCESS_KEY_ID=your_aws_access_key# AWS S3 Configuration for Bulk Data Operations

AWS_SECRET_ACCESS_KEY=your_aws_secret_keyAWS_S3_BUCKET_NAME=your-inventory-bulk-data-bucket

AWS_REGION=us-east-1AWS_S3_REGION=us-east-1



# AWS Cognito Authentication# AWS SQS Configuration for Reliable Notifications

AWS_COGNITO_USER_POOL_ID=your_cognito_user_pool_idAWS_SQS_REGION=us-east-1

AWS_COGNITO_CLIENT_ID=your_cognito_client_idSQS_ENABLE_NOTIFICATIONS=true

AWS_COGNITO_REGION=us-east-1SQS_WORKER_BATCH_SIZE=5

SQS_WORKER_POLLING_INTERVAL=10
# AWS DynamoDB
DYNAMODB_TABLE_NAME=inventory-products

# AWS S3 Storage
AWS_S3_BUCKET_NAME=your-s3-bucket-name
AWS_S3_REGION=us-east-1

# AWS SNS Notifications
AWS_SNS_REGION=us-east-1
SNS_ENABLE_NOTIFICATIONS=true

# AWS SQS Queue Processing
AWS_SQS_REGION=us-east-1
SQS_ENABLE_NOTIFICATIONS=true
SQS_WORKER_BATCH_SIZE=5
SQS_WORKER_POLLING_INTERVAL=10

# Security Settings
JWT_SECRET_KEY=your-secure-jwt-secret-key-here
TOKEN_EXPIRATION_HOURS=24